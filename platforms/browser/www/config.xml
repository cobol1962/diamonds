<?xml version='1.0' encoding='utf-8'?>
<widget id="com.royalcoster.catalog" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Coster Diamonds</name>
    <description>
        Coster Sales App.
    </description>
    <author email="office@royalcoster.com" href="http://phonegap.com">
        Coster Diamonds team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="cordova-plugin-statusbar" source="npm" />
    <gap:plugin name="cordova-plugin-fullscreen" source="npm" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="Fullscreen" value="true" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="KeepRunning" value="true" />
</widget>
